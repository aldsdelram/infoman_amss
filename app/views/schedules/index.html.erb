<h1>List of schedules</h1>

<table class="table">
  <tr>
    <th>Applicant</th>
    <th>Interviewer</th>
    <th>Sched</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @schedules.each do |schedule| %>
  <tr>
    <td><%= schedule.applicant_id %></td>
    <td><%= schedule.interviewer_id %></td>
    <td><%= schedule.sched %></td>
    <td><%= link_to 'Show', schedule %></td>
    <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
    <td><%= link_to 'Destroy', schedule, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>